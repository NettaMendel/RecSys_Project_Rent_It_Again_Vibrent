):\n","  algo,user_splits_df=train_svd(train,user_splits_df, n_factors, n_epochs,lr_all,reg_all,group)\n","  predictions,algo,user_splits_df=test_svd(algo,test,user_splits_df,group)\n","  return predictions,algo,user_splits_df"],"metadata":{"id":"zZ4yHR92cf2z","executionInfo":{"status":"ok","timestamp":1740170019438,"user_tz":-120,"elapsed":3,"user":{"displayName":"Netta Mendelbaum","userId":"17523963483759666592"}}},"execution_count":17,"outputs":[]},{"cell_type":"code","source":["n_factors_list=[50,100,150]\n","n_epochs_list=[10,20,30]\n","lr_all_list=[0.01,0.005,0.001]\n","reg_all_list=[0.05,0.02,0.01]"],"metadata":{"id":"RfWtqjUrdnkh","executionInfo":{"status":"ok","timestamp":1740170019440,"user_tz":-120,"elapsed":3,"user":{"displayName":"Netta Mendelbaum","userId":"17523963483759666592"}}},"execution_count":18,"outputs":[]},{"cell_type":"code","source":["##Hyper-Parameters tuning for outfit\n","\n","test_permutations = list(product(n_factors_list, n_epochs_list,lr_all_list,reg_all_list))\n","\n","for n_factors, n_epochs,lr_all ,reg_all in tqdm(test_permutations):\n","    user_splits_df=user_splits.copy()\n","    predictions,algo,user_splits_df = train_and_test_svd(train,val,user_splits_df, n_factors, n_epochs,lr_all ,reg_all)\n","    param_dict = {'n_factors':n_factors, 'n_epochs':n_epochs,'lr_all':lr_all,'reg_all':reg_all }\n","    user_splits_df, all_dict = evaluate_val_metrics_at_n_outfit(user_splits_df,'SVD_rating', n=10,model_params=param_dict)"],"metadata":{"id":"FaH9apOTeJGC","executionInfo":{"status":"ok","timestamp":1740170077952,"user_tz":-120,"elapsed":58511,"user":{"displayName":"Netta Mendelbaum","userId":"17523963483759666592"}},"colab":{"base_uri":"https://localhost:8080/","height":49,"referenced_widgets":["14752fab22eb4e60b765d673d1a24515","3d7e4c59af6b4f2a8286483be05b1928","4418f7df6be94fa38ed5516b3ef581d7","b0dafffe728c49448a3d1cf6932e8105","5b5da3fe5c01433b89497f283ffa9baa","85fbd8f664c94641b50829fa8a67204d","59a47023fba540a2af7caed7e391e908