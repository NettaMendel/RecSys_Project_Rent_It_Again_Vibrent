set()\n","  predictions = algo.test(testset)\n","  predictions_df=pd.DataFrame(predictions)\n","  results=predictions_df[predictions_df['r_ui']>=2]\n","  results.sort_values('r_ui',ascending=False,inplace=True)\n","  # Merge the DataFrames on customer_id and aggregate the outfit_id into a list\n","  df_merged = user_splits_df.merge(results, left_on='customer_id',right_on='uid', how='left').drop(columns='uid').groupby('customer_id').agg({\n","      **{col: 'first' for col in user_splits_df.columns if col != 'customer_id'},\n","      'iid': lambda x: list(x)\n","  }).reset_index()\n","  # Rename the aggregated column to id_prediction\n","  if group:\n","    df_merged.rename(columns={'iid': 'group_prediction'}, inplace=True)\n","  else:\n","    df_merged.rename(columns={'iid': 'id_prediction'}, inplace=True)\n","  return predictions,algo,df_merged\n","\n","def train_and_test_svdpp(train,test,user_splits_df, n_factors, n_epochs,lr_all=0.007,reg_all=0.02,group=False):\n","  algo,user_splits_df=train_svdpp(train,user_splits_df, n_factors, n_epochs,lr_all,reg_all,group)\n","  predictions,algo,user_splits_df=test_svdpp(algo,test,user_splits_df,group)\n","  return predictions,algo,user_splits_df"],"metadata":{"id":"Bo5pAq7ohdi_","executionInfo":{"status":"ok","timestamp":1740170135132,"user_tz":-120,"elapsed":106,"user":{"displayName":"Netta Mendelbaum","userId":"17523963483759666592"}}},"execution_count":22,"outputs":[]},{"cell_type":"code","source":["n_factors_list=[50,100,150]\n","n_epochs_list=[10,20,30]\n","lr_all_list=[0.01,0.007,0.05]\n","reg_all_list=[0.05,0.02,0.01]"],"metadata":{"id":"ds8YSXnUiIQ4","executionInfo":{"status":"ok","timestamp":1740170135133,"user_tz":-120,"elapsed":4,"user":{"displayName":"Netta Mendelbaum","userId":"17523963483759666592"}}},"execution_count":23,"outputs":[]},{"cell_type":"code","source":["##Hyper-Parameters tuning for outfit\n","test_permutations = list(product(n_factors_list, n_epochs_list,lr_all_list,reg_all_list))\n","\n","for n_f